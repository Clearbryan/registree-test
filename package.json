{
  "name": "registree",
  "version": "1.0.0",
  "description": "Registree.io Web App",
  "main": "main.ts",
  "scripts": {
    "frontend": "cd frontend && npm start",
    "build": "tsc",
    "backend:dev": "nodemon --exec ts-node main.ts",
    "backend:prod": "nodemon dist/main.js",
    "client:install": "cd frontend && npm install",
    "registree:dev": "concurrently \"npm run backend:dev\" \"npm run apollo\" \"npm run frontend\"",
    "registree:prod": "concurrently \"npm run backend:prod\" \"npm run frontend\""
  },
  "keywords": [
    "Registree"
  ],
  "author": "Clearmind Bryan Chetekwe",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@graphql-codegen/typescript-react-query": "^3.5.0",
    "apollo-server": "^3.6.2",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.9",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.13"
  }
}